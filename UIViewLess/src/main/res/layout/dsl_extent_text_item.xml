<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <com.angcyo.uiview.less.widget.RTextView
        android:id="@+id/dark_view"
        style="@style/BaseDarkTextStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical|right"
        android:drawablePadding="@dimen/base_mdpi"
        android:paddingRight="@dimen/base_padding_left"
        app:r_noread_padding_right="-2dp"
        app:r_offset_padding="true"
        tools:drawableRight="@drawable/base_next"
        tools:r_show_noread="true"
        tools:text="发现新版本" />

    <com.angcyo.uiview.less.draw.view.RDrawNoReadNumView
        android:id="@+id/read_num_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical|right"
        android:layout_marginRight="@dimen/base_padding_left"
        app:r_draw_space_horizontal_size="@dimen/base_ldpi"
        tools:r_draw_read_num_string="9" />
</merge>